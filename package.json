    "@mui/lab": "^5.0.0-alpha.51",
    "@mui/material": "^5.0.4",
    "@stripe/stripe-js": "^2.1.5",
    "apexcharts": "^3.35.2",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "clsx": "^1.1.1",
    "cross-env": "^7.0.3",
    "currency.js": "^2.0.4",
    "date-fns": "^2.25.0",
